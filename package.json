{
  "name": "unexpected-snapshot",
  "version": "1.0.0",
  "description": "Add inline snapshotting powers to the Unexpected assertion library",
  "main": "lib/unexpected-snapshot.js",
  "browser": "unexpected-snapshot-browser.js",
  "files": [
    "lib",
    "unexpected-snapshot-browser.js",
    "unexpected-snapshot-browser.js.map"
  ],
  "scripts": {
    "lint": "eslint . && prettier --check '**/*.{js,md}'",
    "test": "mocha",
    "build": "./build.sh",
    "test-headless": "npm run build && karma start --single-run",
    "test:ci": "npm run coverage && npm run test-headless",
    "coverage": "nyc --reporter=lcov --reporter=text --all -- mocha && echo google-chrome coverage/lcov-report/index.html",
    "prepublishOnly": "npm run build",
    "preversion": "offline-github-changelog --next=${npm_package_version} > CHANGELOG.md && git add CHANGELOG.md"
  },
  "author": "Andreas Lind <andreaslindpetersen@gmail.com>",
  "license": "BSD-3-Clause",
  "dependencies": {
    "@gustavnikolaj/string-utils": "^2.0.0",
    "bluebird": "^3.5.0",
    "byline": "^5.0.0",
    "detect-indent": "^6.0.0",
    "eslint": "^6.0.0",
    "is-node": "^1.0.2",
    "lodash.camelcase": "^4.3.0",
    "memoizesync": "^1.1.1",
    "pretty-maybe": "^1.0.0"
  },
  "devDependencies": {
    "buble": "^0.19.7",
    "coveralls": "^3.0.2",
    "escodegen": "^1.11.1",
    "eslint-config-prettier": "^6.0.0",
    "eslint-config-standard": "^14.0.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-mocha": "^6.0.0",
    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.0",
    "espree": "^6.0.0",
    "karma": "^4.1.0",
    "karma-chrome-launcher": "^3.0.0",
    "karma-mocha": "^1.3.0",
    "mocha": "^7.0.0",
    "nyc": "^15.0.0",
    "offline-github-changelog": "^1.6.1",
    "prettier": "~2.0.2",
    "puppeteer": "^2.0.0",
    "rollup": "^2.0.3",
    "rollup-plugin-commonjs": "^10.0.0",
    "rollup-plugin-json": "^4.0.0",
    "rollup-plugin-node-resolve": "^5.0.0",
    "rollup-plugin-terser": "^5.0.0",
    "unassessed": "^0.2.1",
    "unexpected": "^11.1.1"
  },
  "peerDependencies": {
    "unexpected": "^10.27.0 || ^11.0.0"
  },
  "nyc": {
    "include": [
      "lib/**"
    ]
  }
